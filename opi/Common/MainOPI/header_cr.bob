  <widget type="bool_button" version="2.0.0">
    <name>Boolean Button_1</name>
    <pv_name>$(P)-LOCALSCREENLOCKONE</pv_name>
    <x>735</x>
    <y>56</y>
    <off_label>ONE</off_label>
    <off_color>
      <color name="Button_Background" red="236" green="236" blue="236">
      </color>
    </off_color>
    <on_label>ONE</on_label>
    <on_color>
      <color name="GREEN" red="61" green="216" blue="61">
      </color>
    </on_color>
    <show_led>false</show_led>
    <rules>
      <rule name="BckgColor" prop_id="background_color" out_exp="false">
        <exp bool_exp="pv0==0">
          <value>
            <color name="Button_Background" red="236" green="236" blue="236">
            </color>
          </value>
        </exp>
        <exp bool_exp="pv0==1">
          <value>
            <color name="GREEN" red="61" green="216" blue="61">
            </color>
          </value>
        </exp>
        <pv_name>$(pv_name)</pv_name>
      </rule>
      <rule name="Permission" prop_id="enabled" out_exp="false">
        <exp bool_exp="pv0==0 &amp;&amp; pv1==0">
          <value>true</value>
        </exp>
        <exp bool_exp="pv0==1 || pv1==1">
          <value>false</value>
        </exp>
        <pv_name>$(P)-LOCALSCREENLOCKTWO</pv_name>
        <pv_name>$(P)-LOCALSCREENLOCK$(LOCKID)</pv_name>
      </rule>
    </rules>
  </widget>
