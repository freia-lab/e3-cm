      </line_color>
      <background_color>
        <color name="BACKGROUND" red="220" green="225" blue="221">
        </color>
      </background_color>
      <actions>
      </actions>
      <rules>
        <rule name="BackgroundColor" prop_id="background_color" out_exp="false">
          <exp bool_exp="pvInt0==0 &amp;&amp; pv1==1 ">
            <value>
              <color name="ERROR" red="252" green="13" blue="27">
              </color>
            </value>
          </exp>
          <exp bool_exp="pvInt0==0 &amp;&amp; pv1==0">
            <value>
              <color name="BACKGROUND" red="220" green="225" blue="221">
              </color>
            </value>
          </exp>
          <exp bool_exp="pvInt==1">
            <value>
              <color name="BACKGROUND" red="220" green="225" blue="221">
              </color>
            </value>
          </exp>
          <pv_name>$(P)-FS-$(IND):Alarm</pv_name>
          <pv_name>sim://ramp(0,1,1,1)</pv_name>
        </rule>
      </rules>
      <tooltip>$(pv_name)
$(pv_value)</tooltip>
    </widget>
    <widget type="polyline" version="2.0.0">
